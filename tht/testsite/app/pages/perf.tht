
F main() {

    Session.get('nothing', '');

	let rows = [];
    for (num in range(1, 20)) {
        rows #= num ~ '-' ~ String.random(20);
    }

	Web.sendPage({
		body: html(rows),
		title: 'Perf Test Page',
		//css: Css.include('base')
	});
}


T html(rows) {

    <main>
        <h1>> Perf Test Page

        <p>> Date: <b>> {{ Date.now() }}

        <ul>
        :: for (row in rows)  {
            <li>> {{ row }}
        :: }
        </>
    </>

}
